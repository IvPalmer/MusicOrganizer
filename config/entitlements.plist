<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
      <!-- Allow unsigned executable memory only if absolutely necessary (try removing if issues persist) -->
      <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
      <false/>
      
      <!-- Disable library validation so that dynamic libraries not signed by Apple can load -->
      <key>com.apple.security.cs.disable-library-validation</key>
      <true/>
      
      <!-- Enable the hardened runtime (required for notarization) -->
      <key>com.apple.security.hardened-runtime</key>
      <true/>
      
      <!-- Do not include get-task-allow -->
  </dict>
</plist>